{
    "org": "$HZN_ORG_ID",
    "label": "$SERVICE_NAME-$ARCH",
    "url": "$SERVICE_NAME",
    "version": "$SERVICE_VERSION",
    "arch": "$ARCH",
    "public": true,
    "sharable": "singleton",
    "requiredServices": [],
    "userInput": [],
    "deployment": {
	"services": {
	    "$SERVICE_NAME": {
		"image": "${DOCKER_IMAGE_BASE}_$ARCH:$SERVICE_VERSION",
		"ports": [
		    {
			"HostPort": "7778:7778/tcp",
			"HostIP": "0.0.0.0"
		    }
		],
		"network": "host"
	    }
	}
    }
}

