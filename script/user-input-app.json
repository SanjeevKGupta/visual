{
    "services": [
    {
	"org": "$HZN_ORG_ID",
	"url": "$EDGE_OWNER.$EDGE_DEPLOY.http",
	"versionRange": "[0.0.0,INFINITY)",
	"variables": {
	    "APP_IEAM_URL"                : "$APP_IEAM_URL",
	    "APP_APIKEY_PASSWORD"         : "$APP_APIKEY_PASSWORD",
	    "APP_MMS_OBJECT_TYPE_CONFIG"  : "$APP_MMS_OBJECT_TYPE_CONFIG",
	    "APP_MMS_OBJECT_ID_CONFIG"    : "$APP_MMS_OBJECT_ID_CONFIG",
            "EVENTSTREAMS_BROKER_URLS"    : "$EVENTSTREAMS_BROKER_URLS",
            "EVENTSTREAMS_API_KEY"        : "$EVENTSTREAMS_API_KEY",
            "EVENTSTREAMS_ENHANCED_TOPIC" : "$EVENTSTREAMS_ENHANCED_TOPIC"
	}
    },
    {
	"org": "$HZN_ORG_ID",
	"url": "$EDGE_OWNER.$EDGE_DEPLOY.tflite",
	"versionRange": "[0.0.0,INFINITY)",
	"variables": {
            "DEVICE_ID"                : "$DEVICE_ID",
            "DEVICE_NAME"              : "$DEVICE_NAME",
	    "DEVICE_IP_ADDRESS"        : "$DEVICE_IP_ADDRESS",
	    "SHOW_OVERLAY"             : $SHOW_OVERLAY,
	    "PUBLISH_KAFKA"            : $PUBLISH_KAFKA,
	    "PUBLISH_STREAM"           : $PUBLISH_STREAM,
	    "DETECT_FACE"              : $DETECT_FACE,
	    "BLUR_FACE"                : $BLUR_FACE,
	    "MIN_CONFIDENCE_THRESHOLD" : 0.6
	}
    }
  ]
}
